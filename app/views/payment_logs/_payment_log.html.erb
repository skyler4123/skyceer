<div id="<%= dom_id payment_log %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Payment user:</strong>
    <%= payment_log.payment_user_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Payment customer:</strong>
    <%= payment_log.payment_customer_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Payment order:</strong>
    <%= payment_log.payment_order_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Payment method:</strong>
    <%= payment_log.payment_method_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Payment discount:</strong>
    <%= payment_log.payment_discount_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Payment invoice:</strong>
    <%= payment_log.payment_invoice_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Action:</strong>
    <%= payment_log.action %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Amount:</strong>
    <%= payment_log.amount %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Note:</strong>
    <%= payment_log.note %>
  </p>

</div>
