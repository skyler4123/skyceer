<div id="<%= dom_id payment_category_appointment %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Payment category:</strong>
    <%= payment_category_appointment.payment_category_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Payment customer:</strong>
    <%= payment_category_appointment.payment_customer_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Payment method:</strong>
    <%= payment_category_appointment.payment_method_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Payment discount:</strong>
    <%= payment_category_appointment.payment_discount_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Payment item:</strong>
    <%= payment_category_appointment.payment_item_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Payment order:</strong>
    <%= payment_category_appointment.payment_order_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Payment invoice:</strong>
    <%= payment_category_appointment.payment_invoice_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Discarded at:</strong>
    <%= payment_category_appointment.discarded_at %>
  </p>

</div>
