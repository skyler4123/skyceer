<div id="<%= dom_id payment_item %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Payment user:</strong>
    <%= payment_item.payment_owner_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Payment itemable:</strong>
    <%= payment_item.payment_itemable_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Price:</strong>
    <%= payment_item.price %>
  </p>

</div>
