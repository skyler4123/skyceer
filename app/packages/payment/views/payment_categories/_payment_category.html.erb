<div id="<%= dom_id payment_category %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= payment_category.name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Payment user:</strong>
    <%= payment_category.payment_user_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Parent category:</strong>
    <%= payment_category.parent_category_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Discarded at:</strong>
    <%= payment_category.discarded_at %>
  </p>

</div>
