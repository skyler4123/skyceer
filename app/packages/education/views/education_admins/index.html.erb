<div class="w-full">
  <% if notice.present? %>
    <p class="py-2 px-3 bg-green-50 mb-5 text-green-500 font-medium rounded-lg inline-block" id="notice"><%= notice %></p>
  <% end %>

  <% content_for :title, "Education admins" %>

  <div class="flex justify-between items-center">
    <h1 class="font-bold text-4xl">Education admins</h1>
    <%= link_to "New education admin", new_education_admin_path, class: "rounded-lg py-3 px-5 bg-blue-600 text-white block font-medium" %>
  </div>

  <div id="education_admins" class="min-w-full">
    <% @education_admins.each do |education_admin| %>
      <%= render education_admin %>
      <p>
        <%= link_to "Show this education admin", education_admin, class: "ml-2 rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
      </p>
    <% end %>
  </div>
</div>
