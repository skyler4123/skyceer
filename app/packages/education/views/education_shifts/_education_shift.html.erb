<div id="<%= dom_id education_shift %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Title:</strong>
    <%= education_shift.title %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Location:</strong>
    <%= education_shift.location %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Start time:</strong>
    <%= education_shift.start_time %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">End time:</strong>
    <%= education_shift.end_time %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Duration:</strong>
    <%= education_shift.duration %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Education school:</strong>
    <%= education_shift.education_school_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Education class:</strong>
    <%= education_shift.education_class_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Education subject:</strong>
    <%= education_shift.education_subject_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Education teacher:</strong>
    <%= education_shift.education_teacher_id %>
  </p>

</div>
