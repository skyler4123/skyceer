<div class="mx-auto w-4/5 flex">
  <div class="mx-auto">
    <%= render @education_admin %>

    <%= link_to "Edit this education admin", edit_education_admin_path(@education_admin), class: "mt-2 rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to "Back to education admins", education_admins_path, class: "ml-2 rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <div class="inline-block ml-2">
      <%= button_to "Destroy this education admin", @education_admin, method: :delete, class: "mt-2 rounded-lg py-3 px-5 bg-gray-100 font-medium" %>
