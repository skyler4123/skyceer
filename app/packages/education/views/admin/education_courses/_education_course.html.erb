<div id="<%= dom_id education_course %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= education_course.name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Duration:</strong>
    <%= education_course.duration %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Datetime:</strong>
    <%= education_course.datetime %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Description:</strong>
    <%= education_course.description %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Contact:</strong>
    <%= education_course.contact %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Email:</strong>
    <%= education_course.email %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Img url:</strong>
    <%= education_course.img_url %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Education:</strong>
    <%= education_course.education_id %>
  </p>

</div>
