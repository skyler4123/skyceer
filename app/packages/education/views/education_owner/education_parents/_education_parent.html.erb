<div id="<%= dom_id education_parent %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">User:</strong>
    <%= education_parent.user_id %>
  </p>
  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= education_parent.name %>
  </p>
  <p class="my-5">
    <strong class="block font-medium mb-1">Email:</strong>
    <%= education_parent.email %>
  </p>
  <p class="my-5">
    <strong class="block font-medium mb-1">Discarded at:</strong>
    <%= education_parent.discarded_at %>
  </p>
</div>
