<div id="<%= dom_id education_lesson %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Title:</strong>
    <%= education_lesson.title %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Content:</strong>
    <%= education_lesson.content %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Education school:</strong>
    <%= education_lesson.education_school_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Education class:</strong>
    <%= education_lesson.education_class_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Education subject:</strong>
    <%= education_lesson.education_subject_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Education teacher:</strong>
    <%= education_lesson.education_teacher_id %>
  </p>

</div>
