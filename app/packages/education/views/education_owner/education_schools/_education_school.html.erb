<div id="<%= dom_id education_school %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">User:</strong>
    <%= education_school.user.name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Address:</strong>
    <%= education_school.address_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= education_school.name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Email:</strong>
    <%= education_school.email %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Category:</strong>
    <%= education_school.category %>
  </p>

</div>
