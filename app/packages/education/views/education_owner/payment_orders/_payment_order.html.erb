<div id="<%= dom_id payment_order %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Payment user:</strong>
    <%= payment_order.payment_owner_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Payment customer:</strong>
    <%= payment_order.payment_customer_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Payment method:</strong>
    <%= payment_order.payment_method_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Payment discount:</strong>
    <%= payment_order.payment_discount_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Status:</strong>
    <%= payment_order.status %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Amount:</strong>
    <%= payment_order.amount %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Paid:</strong>
    <%= payment_order.paid %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Due:</strong>
    <%= payment_order.due %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Expire:</strong>
    <%= payment_order.expire %>
  </p>

</div>
