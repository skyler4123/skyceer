<div id="<%= dom_id education_class %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Education school id:</strong>
    <%= education_class.education_school_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Education school name:</strong>
    <%= education_class.education_school.name %>
  </p>


  <p class="my-5">
    <strong class="block font-medium mb-1">Education course:</strong>
    <%= education_class.education_course.name%>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= education_class.name %>
  </p>

</div>
