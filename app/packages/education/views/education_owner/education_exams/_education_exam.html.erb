<div id="<%= dom_id education_exam %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= education_exam.name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Description:</strong>
    <%= education_exam.description %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Status:</strong>
    <%= education_exam.status %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Education school:</strong>
    <%= education_exam.education_school_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Education subject:</strong>
    <%= education_exam.education_subject_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Discarded at:</strong>
    <%= education_exam.discarded_at %>
  </p>

</div>
