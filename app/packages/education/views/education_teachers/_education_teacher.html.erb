<div id="<%= dom_id education_teacher %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Education user:</strong>
    <%= education_teacher.education_user_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= education_teacher.name %>
  </p>

  <% if action_name != "show" %>
    <%= link_to "Show this education teacher", education_teacher, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to "Edit this education teacher", edit_education_teacher_path(education_teacher), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
    <hr class="mt-6">
  <% end %>
</div>
