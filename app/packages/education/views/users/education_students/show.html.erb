<div class="mx-auto md:w-2/3 w-full flex">
  <div class="mx-auto">
    <%= render @education_student %>

    <%= link_to "Edit this education student", edit_education_student_path(@education_student), class: "mt-2 rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to "Back to education students", education_students_path, class: "ml-2 rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <div class="inline-block ml-2">
      <%= button_to "Destroy this education student", @education_student, method: :delete, class: "mt-2 rounded-lg py-3 px-5 bg-gray-100 font-medium" %>
    </div>
  </div>
</div>
