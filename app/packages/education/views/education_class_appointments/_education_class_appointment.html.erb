<div id="<%= dom_id education_class_appointment %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Education class:</strong>
    <%= education_class_appointment.education_class_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Education teacher:</strong>
    <%= education_class_appointment.education_teacher_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Education student:</strong>
    <%= education_class_appointment.education_student_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Education subject:</strong>
    <%= education_class_appointment.education_subject_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Education room:</strong>
    <%= education_class_appointment.education_room_id %>
  </p>

</div>
