<div class="w-full">
  <% if notice.present? %>
    <p class="w-full text-center py-2 px-3 bg-green-50 mb-5 text-green-500 font-medium rounded-lg inline-block" id="notice"><%= notice %></p>
  <% end %>

  <% content_for :title, "Education teachers" %>

  <div class="flex justify-between items-center">
    <h1 class="font-bold text-4xl">Education teachers ssssss</h1>
    <%= link_to "New education teacher", new_education_school_education_teacher_path, class: "rounded-lg py-3 px-5 bg-blue-600 text-white block font-medium" %>
  </div>

  <div id="education_teachers" class="min-w-full">
    <% @education_teachers.each do |education_teacher| %>
      <%= render education_teacher %>
      <p>
        <%= link_to "Show this education teacher", education_teacher, class: "ml-2 rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
      </p>
    <% end %>
  </div>
</div>
