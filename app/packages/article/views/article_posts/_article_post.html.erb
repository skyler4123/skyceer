<div id="<%= dom_id article_post %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">User:</strong>
    <%= article_post.user.username %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Title:</strong>
    <%= article_post.title %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Content:</strong>
    <%= article_post.content %>
  </p>

</div>
