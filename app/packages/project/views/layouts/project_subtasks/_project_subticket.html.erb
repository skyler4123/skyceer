<div id="<%= dom_id project_subtask %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Project task:</strong>
    <%= project_subtask.project_task_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= project_subtask.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= project_subtask.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Status:</strong>
    <%= project_subtask.status %>
  </div>
</div>
