<div id="<%= dom_id project_task %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Project group:</strong>
    <%= project_task.project_group_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= project_task.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= project_task.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Status:</strong>
    <%= project_task.status %>
  </div>
</div>
