<div id="<%= dom_id project_group %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Project group ownerable:</strong>
    <%= project_group.project_ownerable_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= project_group.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= project_group.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Status:</strong>
    <%= project_group.status %>
  </div>
</div>
