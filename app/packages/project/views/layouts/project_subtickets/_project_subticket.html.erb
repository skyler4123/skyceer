<div id="<%= dom_id project_subticket %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Project ticket:</strong>
    <%= project_subticket.project_ticket_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= project_subticket.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= project_subticket.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Status:</strong>
    <%= project_subticket.status %>
  </div>
</div>
