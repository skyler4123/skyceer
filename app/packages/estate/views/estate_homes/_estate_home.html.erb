<div id="<%= dom_id estate_home %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Estate user:</strong>
    <%= estate_home.estate_user_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= estate_home.name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Address:</strong>
    <%= estate_home.address %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Price cents:</strong>
    <%= estate_home.price_cents %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Longitude:</strong>
    <%= estate_home.longitude %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Latitude:</strong>
    <%= estate_home.latitude %>
  </p>

</div>
