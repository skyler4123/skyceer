<div id="<%= dom_id estate_hotel %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">User:</strong>
    <%= estate_hotel.user_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= estate_hotel.name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Address:</strong>
    <%= estate_hotel.address_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Price cents:</strong>
    <%= estate_hotel.price_cents %>
  </p>

  <% if estate_hotel.images.attached? %>
    <% estate_hotel.images.each do |image| %>
      <%= image_tag image.variant(:full) %>
    <% end %>
  <% end %>
</div>
