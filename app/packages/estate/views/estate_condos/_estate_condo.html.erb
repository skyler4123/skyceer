<div id="<%= dom_id estate_condo %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Estate user:</strong>
    <%= estate_condo.estate_user_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= estate_condo.name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Address:</strong>
    <%= estate_condo.address %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Price cents:</strong>
    <%= estate_condo.price_cents %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Longitude:</strong>
    <%= estate_condo.longitude %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Latitude:</strong>
    <%= estate_condo.latitude %>
  </p>

  <% if estate_condo.images.attached? %>
    <% estate_condo.images.each do |image| %>
      <%= image_tag image.variant(:full) %>
    <% end %>
  <% end %>
</div>
