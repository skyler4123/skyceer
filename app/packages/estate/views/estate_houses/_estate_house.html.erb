<div id="<%= dom_id estate_house %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Estate user:</strong>
    <%= estate_house.estate_user_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= estate_house.name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Address:</strong>
    <%= estate_house.address %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Price cents:</strong>
    <%= estate_house.price_cents %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Longitude:</strong>
    <%= estate_house.longitude %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Latitude:</strong>
    <%= estate_house.latitude %>
  </p>

</div>
