<div id="<%= dom_id agriculture_farm %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Agriculture farmer:</strong>
    <%= agriculture_farm.agriculture_farmer_id %>
  </p>

  <% if action_name != "show" %>
    <%= link_to "Show this agriculture farm", agriculture_farm, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to "Edit this agriculture farm", edit_agriculture_farm_path(agriculture_farm), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
    <hr class="mt-6">
  <% end %>
</div>
