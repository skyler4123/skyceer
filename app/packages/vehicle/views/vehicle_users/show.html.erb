<div class="mx-auto md:w-2/3 w-full flex">
  <div class="mx-auto">
    <%= render @vehicle_user %>

    <%= link_to "Edit this vehicle_user", edit_vehicle_user_path(@vehicle_user), class: "mt-2 rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <div class="inline-block ml-2">
      <%= button_to "Destroy this vehicle_user", vehicle_user_path(@vehicle_user), method: :delete, class: "mt-2 rounded-lg py-3 px-5 bg-gray-100 font-medium" %>
    </div>
    <%= link_to "Back to vehicle_users", vehicle_users_path, class: "ml-2 rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
  </div>
</div>
