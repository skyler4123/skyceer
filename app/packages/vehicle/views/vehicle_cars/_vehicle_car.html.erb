<div id="<%= dom_id vehicle_car %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= vehicle_car.name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Model:</strong>
    <%= vehicle_car.model %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Vehicle brand:</strong>
    <%= vehicle_car.brand %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Vehicle user:</strong>
    <%= vehicle_car.vehicle_user_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Price:</strong>
    <%= vehicle_car.price %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Version:</strong>
    <%= vehicle_car.version %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Coordinates:</strong>
    <%= vehicle_car.coordinates %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Released at:</strong>
    <%= vehicle_car.released_at %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Verified:</strong>
    <%= vehicle_car.verified %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Expired:</strong>
    <%= vehicle_car.expired %>
  </p>

  <% if action_name != "show" %>
    <%= link_to "Show this car car", vehicle_car, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to "Edit this car car", edit_vehicle_car_path(vehicle_car), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
    <hr class="mt-6">
  <% end %>
</div>
