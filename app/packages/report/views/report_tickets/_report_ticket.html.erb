<div id="<%= dom_id report_ticket %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Title:</strong>
    <%= report_ticket.title %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Content:</strong>
    <%= report_ticket.content %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Category:</strong>
    <%= report_ticket.category %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Status:</strong>
    <%= report_ticket.status %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Reporter email:</strong>
    <%= report_ticket.reporter_email %>
  </p>

</div>
