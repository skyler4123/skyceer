<div id="<%= dom_id report_ticket %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Report user:</strong>
    <%= report_ticket.report_user_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Title:</strong>
    <%= report_ticket.title %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Content:</strong>
    <%= report_ticket.content %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Status:</strong>
    <%= report_ticket.status %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Email:</strong>
    <%= report_ticket.email %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Phone:</strong>
    <%= report_ticket.phone %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Nation:</strong>
    <%= report_ticket.nation %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Discarded at:</strong>
    <%= report_ticket.discarded_at %>
  </p>

</div>
