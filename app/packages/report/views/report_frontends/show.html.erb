<div class="mx-auto w-4/5 flex">
  <div class="mx-auto">
    <%= render @report_frontend %>

    <%= link_to "Edit this report frontend", edit_report_frontend_path(@report_frontend), class: "mt-2 rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to "Back to report frontends", report_frontends_path, class: "ml-2 rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <div class="inline-block ml-2">
      <%= button_to "Destroy this report frontend", @report_frontend, method: :delete, class: "mt-2 rounded-lg py-3 px-5 bg-gray-100 font-medium" %>
    </div>
  </div>
</div>
