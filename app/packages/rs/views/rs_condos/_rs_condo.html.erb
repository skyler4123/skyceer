<div id="<%= dom_id rs_condo %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Rs user:</strong>
    <%= rs_condo.rs_user_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= rs_condo.name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Address:</strong>
    <%= rs_condo.address %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Coordinates:</strong>
    <%= rs_condo.coordinates %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Price cents:</strong>
    <%= rs_condo.price_cents %>
  </p>

</div>
