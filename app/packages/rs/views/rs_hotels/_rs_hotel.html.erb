<div id="<%= dom_id rs_hotel %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Rs user:</strong>
    <%= rs_hotel.rs_user_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= rs_hotel.name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Address:</strong>
    <%= rs_hotel.address %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Price cents:</strong>
    <%= rs_hotel.price_cents %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Longitude:</strong>
    <%= rs_hotel.longitude %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Latitude:</strong>
    <%= rs_hotel.latitude %>
  </p>

</div>
