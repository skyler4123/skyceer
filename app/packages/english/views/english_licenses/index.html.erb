<div class="w-full">
  <% if notice.present? %>
    <p class="py-2 px-3 bg-green-50 mb-5 text-green-500 font-medium rounded-lg inline-block" id="notice"><%= notice %></p>
  <% end %>

  <% content_for :title, "English licenses" %>

  <div class="flex justify-between items-center">
    <h1 class="font-bold text-4xl">English licenses</h1>
    <%= link_to "New english license", new_english_license_path, class: "rounded-lg py-3 px-5 bg-blue-600 text-white block font-medium" %>
  </div>

  <div id="english_licenses" class="min-w-full">
    <% @english_licenses.each do |english_license| %>
      <%= render english_license %>
      <p>
        <%= link_to "Show this english license", english_license, class: "ml-2 rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
      </p>
    <% end %>
  </div>
</div>
