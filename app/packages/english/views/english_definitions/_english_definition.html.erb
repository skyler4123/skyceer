<div id="<%= dom_id english_definition %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Definition:</strong>
    <%= english_definition.definition %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Synonyms:</strong>
    <%= english_definition.synonyms %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Antonyms:</strong>
    <%= english_definition.antonyms %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Example:</strong>
    <%= english_definition.example %>
  </p>

</div>
