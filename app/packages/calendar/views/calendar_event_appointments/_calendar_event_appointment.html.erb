<div id="<%= dom_id calendar_event_appointment %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Calendar event:</strong>
    <%= calendar_event_appointment.calendar_event_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Calendar event appointmentable:</strong>
    <%= calendar_event_appointment.calendar_event_appointmentable_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Discarded at:</strong>
    <%= calendar_event_appointment.discarded_at %>
  </p>

</div>
