<%= button(type: :none, action: {listener: :click, action: :dark_mode}, event: { id: new_id, action: :tab_next, listener: :click }) do %>
  <%= tab(event: { id: id }) do |c| %>
    <div><%= icon(name: "moon", klass: 'fill-blue-950') %></div>
    <div><%= icon(name: "sun") %></div>
  <% end %>
<% end %>